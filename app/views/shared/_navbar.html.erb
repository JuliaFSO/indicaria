<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%= link_to "#", class: "navbar-brand" do %>
    <%= image_tag "https://res.cloudinary.com/dsjv33crm/image/upload/v1623324507/indicaria/logo_shadow_sfqgfg.png" %>
  <% end %>

<%= simple_form_for :search, url: root_path, method: "GET", html: { class: 'form-inline' } do |f| %>
  <%= f.input "Search", label: false, placeholder: "Buscar" %>
  <%= f.submit ("<i class='fa fa-search'</i> Search"), :class => "btn btn-primary" %>
<% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item active align-self-center">
        <%= link_to "Cadastre-se", new_user_session_path, class: "navbar-link" %>
      </li>
    </ul>
  </div>
</div>
